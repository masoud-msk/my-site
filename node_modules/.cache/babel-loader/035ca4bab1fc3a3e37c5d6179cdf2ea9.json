"(character, index) in characters\"\n      :key=\"index\"\n      :style=\"{ color: calculateColor(index) }\">{{ character }}</span>\n    <br />\n    <br />Press Ctrl + c to leave.\n  </div>\n</template>\n\n<script>\nimport { C_KEY } from '../constants/keys'\n\nconst COLORS = [\n  '#FF0000',\n  '#FF9900',\n  '#CCFF00',\n  '#33FF00',\n  '#00FF66',\n  '#00FFFF',\n  '#0066FF',\n  '#3300FF',\n  '#CC00FF',\n  '#FF0099'\n]\nconst INTERVAL_TIMEOUT = 40\n\nexport default {\n  inject: ['terminate'],\n\n  data: () => ({\n    characters: 'SAJAD ADINEH',\n    index: 0,\n    interval: undefined\n  }),\n\n  mounted () {\n    this.interval = setInterval(() => {\n      this.index++\n    }, INTERVAL_TIMEOUT)\n\n    window.addEventListener(\n      'keydown',\n      (event) => {\n        // Check for \"Ctrl\" and \"c\" simultaneously\n        if (event.ctrlKey && event.keyCode === C_KEY) {\n          // Stop animation\n          clearInterval(this.interval)\n\n          this.terminate()\n        }\n      },\n      true\n    )\n  },\n\n  methods: {\n    // Returns color by given index\n    calculateColor (index) {\n      index += this.index\n      const offset = Math.floor(index / COLORS.length) * COLORS.length\n\n      return COLORS[index - offset]\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/hosted"}]}