O,MAAMc,YAAY,GAAG,CAACd,OAAD,EAAUE,YAAY,GAAG,KAAzB,EAAgCC,IAAI,GAAG,kBAAvC,EAA2D,GAAGC,MAA9D,MAA0E;AACpGD,EAAAA,IADoG;AAEpGC,EAAAA,MAFoG;AAGpGC,EAAAA,MAAM,EAAE,CAAC,WAAD,CAH4F;;AAIpG,QAAMC,OAAN,GAAiB;AACf;AACA,UAAM,KAAKC,SAAL,EAAN;AAEA,SAAKC,SAAL;AACD,GATmG;;AAWpGC,EAAAA,MAAM,EAAEC,aAAa,IAAI;AACvB,QAAIR,YAAJ,EAAkB;AAChB,aAAOQ,aAAa,CAAC,MAAD,EAAS,EAAT,EAAaV,OAAb,CAApB;AACD;;AAED,WAAOU,aAAa,CAAC,MAAD,EAAS;AAAEC,MAAAA,QAAQ,EAAE;AAAEE,QAAAA,SAAS,EAAEb;AAAb;AAAZ,KAAT,CAApB;AACD;AAjBmG,CAA1E,CAArB,C,CAoBP;;AACA,OAAO,MAAMe,iBAAiB,GAAG,CAACZ,IAAI,GAAG,uBAAR,EAAiC,GAAGC,MAApC,MAAgD;AAC/ED,EAAAA,IAD+E;AAE/EC,EAAAA,MAF+E;AAG/EC,EAAAA,MAAM,EAAE,CAAC,WAAD,CAHuE;;AAI/E,QAAMC,OAAN,GAAiB;AACf;AACA,UAAM,KAAKC,SAAL,EAAN;AAEA,SAAKC,SAAL;AACD,GAT8E;;AAW/EC,EAAAA,MAAM,EAAEC,aAAa,IAAIA,aAAa,CAAC,MAAD,EAAS,EAAT,EAAa,EAAb;AAXyC,CAAhD,CAA1B,C,CAcP;;AACA,OAAO,MAAMM,eAAe,GAAG;AAC7B;AACAC,EAAAA,YAAY,EAAEC,QAAQ,IAAI;AACxBA,IAAAA,QAAQ,CAACC,KAAT,CAAe,WAAf,EAA4BC,gBAA5B,CAA6C,SAA7C,EAAwDC,KAAK,IAAI;AAC/D,UAAIA,KAAK,CAACC,OAAN,KAAkBxB,OAAlB,IAA6B,CAACoB,QAAQ,CAACK,KAAT,CAAeC,YAAjD,EAA+D;AAC7DH,QAAAA,KAAK,CAACI,cAAN;AAEAP,QAAAA,QAAQ,CAACD,YAAT;AACD;AACF,KAND;AAOD,GAV4B;AAY7B;AACAS,EAAAA,OAAO,EAAER,QAAQ,IAAI;AACnBA,IAAAA,QAAQ,CAACC,KAAT,CAAe,WAAf,EAA4BC,gBAA5B,CAA6C,SAA7C,EAAwDC,KAAK,IAAI;AAC/D,UAAIH,QAAQ,CAACK,KAAT,CAAeC,YAAnB,EAAiC;AAC/B;AACD;;AAED,UAAIH,KAAK,CAACC,OAAN,KAAkB3B,YAAtB,EAAoC;AAClC0B,QAAAA,KAAK,CAACI,cAAN;AAEAP,QAAAA,QAAQ,CAACS,eAAT;AACD;;AAED,UAAIN,KAAK,CAACC,OAAN,KAAkB1B,cAAtB,EAAsC;AACpCyB,QAAAA,KAAK,CAACI,cAAN;AAEAP,QAAAA,QAAQ,CAACU,eAAT;AACD;AACF,KAhBD;AAiBD,GA/B4B;AAiC7B;AACAC,EAAAA,MAAM,EAAEX,QAAQ,IAAI;AAClBA,IAAAA,QAAQ,CAACC,KAAT,CAAe,WAAf,EAA4BC,gBAA5B,CAA6C,SAA7C,EAAwDC,KAAK,IAAI;AAC/D,UACEA,KAAK,CAACS,OAAN,IACAT,KAAK,CAACC,OAAN,KAAkBzB,KADlB,IAEA,CAACqB,QAAQ,CAACK,KAAT,CAAeC,YAHlB,EAIE;AACAH,QAAAA,KAAK,CAACI,cAAN;AAEAP,QAAAA,QAAQ,CAACa,kBAAT;AACD;AACF,KAVD;AAWD;AA9C4B,CAAxB;AAiDP,eAAerC,UAAf","sourcesContent":["import VueCommand from './components/VueCommand'\nimport { ARROW_UP_KEY, ARROW_DOWN_KEY, R_KEY, TAB_KEY } from '../src/constants/keys'\n\n// Returns a Stdout component containing a span element with given inner content\nexport const createStdout = (content, isInnerText = false, isEscapeHtml = false, name = 'VueCommandStdout', ...mixins) => ({\n  name,\n  mixins,\n  inject: ['terminate'],\n  async mounted () {\n    // Wait for user mutations\n    await this.$nextTick()\n\n    this.terminate()\n  },\n\n  render: createElement => {\n    if (isEscapeHtml) {\n      return createElement('span', {}, content)\n    }\n\n    if (isInnerText) {\n      return createElement('span', { domProps: { innerText: content } })\n    }\n\n    return createElement('span', { domProps: { innerHTML: content } })\n  }\n})\n\n// Returns a Stderr component containing a span element with given inner content\nexport const createStderr = (content, isEscapeHtml = false, name = 'VueCommandStderr', ...mixins) => ({\n  name,\n  mixins,\n  inject: ['terminate'],\n  async mounted () {\n    // Wait for user mutations\n    await this.$nextTick()\n\n    this.terminate()\n  },\n\n  render: createElement => {\n    if (isEscapeHtml) {\n      return createElement('span', {}, content)\n    }\n\n    return createElement('span', { domProps: { innerHTML: content } })\n  }\n})\n\n// Returns a dummy Stdout component to not show a Stdin\nexport const createDummyStdout = (name = 'VueCommandDummyStdout', ...mixins) => ({\n  name,\n  mixins,\n  inject: ['terminate'],\n  async mounted () {\n    // Wait for user mutations\n    await this.$nextTick()\n\n    this.terminate()\n  },\n\n  render: createElement => createElement('span', {}, '')\n})\n\n// Default event listeners to opt-in\nexport const EVENT_LISTENERS = {\n  // Autocompletion when pressing \"Tab\" key\n  autocomplete: terminal => {\n    terminal.$refs['term-cont'].addEventListener('keydown', event => {\n      if (event.keyCode === TAB_KEY && !terminal.local.isInProgress) {\n        event.preventDefault()\n\n        terminal.autocomplete()\n      }\n    })\n  },\n\n  // Cycle through history with \"Arrow up key\" and \"Arrow down key\"\n  history: terminal => {\n    terminal.$refs['term-cont'].addEventListener('keydown', event => {\n      if (terminal.local.isInProgress) {\n        return\n      }\n\n      if (event.keyCode === ARROW_UP_KEY) {\n        event.preventDefault()\n\n        terminal.decreaseHistory()\n      }\n\n      if (event.keyCode === ARROW_DOWN_KEY) {\n        event.preventDefault()\n\n        terminal.increaseHistory()\n      }\n    })\n  },\n\n  // Search history with \"Ctrl\" and \"r\"\n  search: terminal => {\n    terminal.$refs['term-cont'].addEventListener('keydown', event => {\n      if (\n        event.ctrlKey &&\n        event.keyCode === R_KEY &&\n        !terminal.local.isInProgress\n      ) {\n        event.preventDefault()\n\n        terminal.setIsSearchHandler()\n      }\n    })\n  }\n}\n\nexport default VueCommand\n"]}]}